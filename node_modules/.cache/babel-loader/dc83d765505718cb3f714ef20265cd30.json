AZA,CAAA","sourcesContent":["<template>\n    <div id=\"upload-image\">\n        <h1 class=\"text-center\" style=\"text-decoration: underline\">Upload Beard Image</h1>\n        <br>\n        <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\n            <b-card bg-variant=\"light\">\n                <b-form-group\n                    label-cols=\"2\"\n                    label=\"Title:\"\n                    label-size=\"lg\"\n                    label-align=\"left\"\n                    label-for=\"title\"\n                >\n                    <b-form-input id=\"title\" type=\"text\" placeholder=\"Enter a Title\" size=\"lg\"></b-form-input>\n                </b-form-group>\n                <b-form-group\n                    label-cols=\"2\"\n                    label=\"Caption:\"\n                    label-size=\"lg\"\n                    label-align=\"left\"\n                    label-for=\"caption\"\n                >\n                    <b-form-input id=\"caption\" type=\"text\" placeholder=\"Enter a Caption\" size=\"lg\"></b-form-input>\n                </b-form-group>\n                <b-form-group\n                    label-cols=\"2\"\n                    label=\"Beard Date:\"\n                    label-size=\"lg\"\n                    label-align=\"left\"\n                    label-for=\"date\"\n                >\n                    <b-form-datepicker\n                        id=\"date\"\n                        today-button\n                        reset-button\n                        close-button\n                        locale=\"en\"\n                        placeholder=\"Choose a Date\"\n                        class=\"text-left\"\n                        size=\"lg\"\n                    ></b-form-datepicker>\n                </b-form-group>\n                <b-form-group\n                    label-cols=\"2\"\n                    label=\"Beard Photo:\"\n                    label-size=\"lg\"\n                    label-align=\"left\"\n                    label-for=\"image\"\n                >\n                    <b-form-file\n                        id=\"image\"\n                        size=\"lg\"\n                        class=\"text-left\"\n                        :state=\"Boolean(form.image)\"\n                        placeholder=\"Choose a file or drop it here...\"\n                        drop-placeholder=\"Drop file here...\"\n                        v-model=\"form.image\"\n                    ></b-form-file>\n                    <div class=\"mt-3 text-left\">Selected file: {{ form.image ? form.image.name : '' }}</div>\n                </b-form-group>\n                <b-row>\n                    <b-col cols=\"3\" sm=\"1\" class=\"text-right\"><b-button pill type=\"submit\" variant=\"primary\" v-b-modal.validation>Submit</b-button></b-col>\n                    <b-col cols=\"3\" sm=\"1\" class=\"text-left\"><b-button pill type=\"reset\" validation=\"dark\">Reset</b-button></b-col>\n                </b-row>\n            </b-card>\n        </b-form>\n    </div>\n</template>\n\n<script>\nexport default {\n  data () {\n    return {\n      form: {\n        title: '',\n        date: null,\n        caption: '',\n        image: null\n      },\n      show: true\n    }\n  },\n  methods: {\n    onSubmit (evt) {\n      evt.preventDefault()\n      // send to the API\n    },\n    onReset (evt) {\n      evt.preventDefault()\n      this.form.title = ''\n      this.form.date = null\n      this.form.caption = ''\n      this.form.image = null\n      this.show = false\n      this.$nextTick(() => {\n        this.show = true\n      })\n    },\n    handleOk (bvModalEvt) {\n      bvModalEvt.preventDefault()\n      this.$router.push('/gallery')\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}